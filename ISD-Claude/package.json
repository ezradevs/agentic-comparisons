{
  "name": "isd-inventory-dashboard",
  "version": "1.0.0",
  "description": "Small Business Inventory & Sales Dashboard",
  "main": "index.js",
  "scripts": {
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
